{
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true
  },
  "rules": {
    "quotes": [ 1, "single" ],
    "no-undef": 0,
    "global-strict": 0,
    "no-underscore-dangle": 0,
    "key-spacing": 0,
    "no-extra-semi": 1,
    "no-unused-vars": 1,
    "no-console": 0,
    "camelcase": 1,
    "no-trailing-spaces": 1,
    "no-debugger": 1
  }
}
